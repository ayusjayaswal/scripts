emoji[flag: Zambia]="ğŸ‡¿ğŸ‡²" 
emoji[flag: Zimbabwe]="ğŸ‡¿ğŸ‡¼"

setmoji=$(printf '%s\n' "${!emoji[@]}" | sort | dmenu -i -l 30)

if [ "$setmoji" ]; then
    chosen=$(printf '%s\n' "${emoji["${setmoji}"]}")
    printf "$chosen" | xclip -selection clipboard
	notify-send "'$chosen' copied to clipboard." &
else
    echo "Program Terminated" && exit 0
fi
